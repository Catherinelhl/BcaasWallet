<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.obt.bcaaswallet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".base.BcaasApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.aty.BrandActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.aty.LoginActivity" />
        <activity android:name=".ui.aty.MainActivity" />
        <activity android:name=".ui.aty.CreateWalletActivity" />
        <activity android:name=".ui.aty.ImportWalletActivity" />
        <activity android:name=".ui.aty.WalletCreatedSuccessActivity" />
        <activity android:name=".ui.aty.SetPwdForImportWalletActivity" />
        <activity android:name=".ui.aty.CheckWalletInfoActivity" />
        <activity android:name=".ui.aty.AddressManagerActivity" />
        <activity android:name=".ui.aty.InsertAddressActivity" />

    </application>

</manifest>